<?xml version="1.0"?>
<rdf:RDF xmlns="http://storeconnect/"
     xml:base="http://storeconnect/"
     xmlns:schema="http://schema.org/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:ssn="http://www.w3.org/ns/ssn/"
     xmlns:sc="http://storeconnect#"
     xmlns:geosparql="http://www.opengis.net/ont/geosparql#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:sosa="http://www.w3.org/ns/sosa/">
    <owl:Ontology rdf:about="http://storeconnect/">
        <owl:versionIRI rdf:resource="http://storeconnect/1.0/"/>
        <owl:imports rdf:resource="http://www.opengis.net/ont/geosparql"/>
        <owl:imports rdf:resource="http://www.w3.org/ns/ssn/"/>
        <terms:title xml:lang="en">FUI StoreConnect</terms:title>
        <owl:versionInfo xml:lang="en">1.0</owl:versionInfo>
        <terms:creator xml:lang="en">Julien Duribreux</terms:creator>
        <terms:creator xml:lang="en">Aur√©lien Bourdon</terms:creator>
        <terms:creator xml:lang="en">Lionel Seinturier</terms:creator>
        <terms:creator xml:lang="en">Romain Rouvoy</terms:creator>
        <rdfs:seeAlso xml:lang="en">https://www.pole-scs.org/projet/storeconnect</rdfs:seeAlso>
        <terms:description xml:lang="en">Official ontology of the FUI StoreConnect project.</terms:description>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://storeconnect/browses -->

    <owl:ObjectProperty rdf:about="http://storeconnect/browses">
        <rdfs:subPropertyOf rdf:resource="http://storeconnect/storeConnectObjectProperty"/>
        <rdfs:domain rdf:resource="http://storeconnect/MotionSubject"/>
        <rdfs:range rdf:resource="http://storeconnect/Route"/>
    </owl:ObjectProperty>
    


    <!-- http://storeconnect/hasHumanCharacteristic -->

    <owl:ObjectProperty rdf:about="http://storeconnect/hasHumanCharacteristic">
        <rdfs:subPropertyOf rdf:resource="http://storeconnect/storeConnectObjectProperty"/>
        <rdfs:domain rdf:resource="http://storeconnect/Human"/>
        <rdfs:range rdf:resource="http://storeconnect/HumanCharacteristic"/>
    </owl:ObjectProperty>
    


    <!-- http://storeconnect/hasIdentifier -->

    <owl:ObjectProperty rdf:about="http://storeconnect/hasIdentifier">
        <rdfs:subPropertyOf rdf:resource="http://storeconnect/storeConnectObjectProperty"/>
        <rdfs:range rdf:resource="http://storeconnect/Identifier"/>
    </owl:ObjectProperty>
    


    <!-- http://storeconnect/hasInstant -->

    <owl:ObjectProperty rdf:about="http://storeconnect/hasInstant">
        <rdfs:subPropertyOf rdf:resource="http://storeconnect/storeConnectObjectProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2006/time#Instant"/>
    </owl:ObjectProperty>
    


    <!-- http://storeconnect/hasLocation -->

    <owl:ObjectProperty rdf:about="http://storeconnect/hasLocation">
        <rdfs:subPropertyOf rdf:resource="http://storeconnect/storeConnectObjectProperty"/>
        <rdfs:domain rdf:resource="http://storeconnect/MotionEvent"/>
        <rdfs:range rdf:resource="http://storeconnect/Location"/>
    </owl:ObjectProperty>
    


    <!-- http://storeconnect/hasMotionEvent -->

    <owl:ObjectProperty rdf:about="http://storeconnect/hasMotionEvent">
        <rdfs:subPropertyOf rdf:resource="http://storeconnect/storeConnectObjectProperty"/>
        <rdfs:domain rdf:resource="http://storeconnect/Route"/>
        <rdfs:range rdf:resource="http://storeconnect/MotionEvent"/>
    </owl:ObjectProperty>
    


    <!-- http://storeconnect/hasMotionState -->

    <owl:ObjectProperty rdf:about="http://storeconnect/hasMotionState">
        <rdfs:subPropertyOf rdf:resource="http://storeconnect/storeConnectObjectProperty"/>
        <rdfs:domain rdf:resource="http://storeconnect/MotionEvent"/>
        <rdfs:range rdf:resource="http://storeconnect/MotionState"/>
    </owl:ObjectProperty>
    


    <!-- http://storeconnect/hasMotionSubject -->

    <owl:ObjectProperty rdf:about="http://storeconnect/hasMotionSubject">
        <rdfs:subPropertyOf rdf:resource="http://storeconnect/storeConnectObjectProperty"/>
        <rdfs:domain rdf:resource="http://storeconnect/MotionEvent"/>
        <rdfs:range rdf:resource="http://storeconnect/MotionSubject"/>
    </owl:ObjectProperty>
    


    <!-- http://storeconnect/hasOrientation -->

    <owl:ObjectProperty rdf:about="http://storeconnect/hasOrientation">
        <rdfs:subPropertyOf rdf:resource="http://storeconnect/storeConnectObjectProperty"/>
        <rdfs:domain rdf:resource="http://storeconnect/MotionEvent"/>
        <rdfs:range rdf:resource="http://storeconnect/Orientation"/>
    </owl:ObjectProperty>
    


    <!-- http://storeconnect/hasPoint -->

    <owl:ObjectProperty rdf:about="http://storeconnect/hasPoint">
        <rdfs:subPropertyOf rdf:resource="http://storeconnect/storeConnectObjectProperty"/>
        <rdfs:domain rdf:resource="http://storeconnect/Location"/>
        <rdfs:range rdf:resource="http://www.opengis.net/ont/sf#Point"/>
    </owl:ObjectProperty>
    


    <!-- http://storeconnect/hasRefinedTimestamp -->

    <owl:ObjectProperty rdf:about="http://storeconnect/hasRefinedTimestamp">
        <rdfs:subPropertyOf rdf:resource="http://storeconnect/storeConnectObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://storeconnect/hasSensorEventTimestamp -->

    <owl:ObjectProperty rdf:about="http://storeconnect/hasSensorEventTimestamp">
        <rdfs:subPropertyOf rdf:resource="http://storeconnect/storeConnectObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://storeconnect/hasSensorTimestamp -->

    <owl:ObjectProperty rdf:about="http://storeconnect/hasSensorTimestamp">
        <rdfs:subPropertyOf rdf:resource="http://storeconnect/storeConnectObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://storeconnect/hasShopArea -->

    <owl:ObjectProperty rdf:about="http://storeconnect/hasShopArea">
        <rdfs:subPropertyOf rdf:resource="http://storeconnect/storeConnectObjectProperty"/>
        <rdfs:domain rdf:resource="http://storeconnect/Store"/>
        <rdfs:range rdf:resource="http://storeconnect/StoreArea"/>
    </owl:ObjectProperty>
    


    <!-- http://storeconnect/hasSpeed -->

    <owl:ObjectProperty rdf:about="http://storeconnect/hasSpeed">
        <rdfs:subPropertyOf rdf:resource="http://storeconnect/storeConnectObjectProperty"/>
        <rdfs:domain rdf:resource="http://storeconnect/MotionEvent"/>
        <rdfs:range rdf:resource="http://storeconnect/Speed"/>
    </owl:ObjectProperty>
    


    <!-- http://storeconnect/hasStoreConnectTimestamp -->

    <owl:ObjectProperty rdf:about="http://storeconnect/hasStoreConnectTimestamp">
        <rdfs:subPropertyOf rdf:resource="http://storeconnect/storeConnectObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://storeconnect/hasTimestamp -->

    <owl:ObjectProperty rdf:about="http://storeconnect/hasTimestamp">
        <rdfs:subPropertyOf rdf:resource="http://storeconnect/storeConnectObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://storeconnect/isWithin -->

    <owl:ObjectProperty rdf:about="http://storeconnect/isWithin">
        <rdfs:subPropertyOf rdf:resource="http://storeconnect/storeConnectObjectProperty"/>
        <rdfs:domain rdf:resource="http://storeconnect/Location"/>
        <rdfs:range rdf:resource="http://storeconnect/StoreArea"/>
    </owl:ObjectProperty>
    


    <!-- http://storeconnect/produces -->

    <owl:ObjectProperty rdf:about="http://storeconnect/produces">
        <rdfs:subPropertyOf rdf:resource="http://storeconnect/storeConnectObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://storeconnect/storeConnectObjectProperty -->

    <owl:ObjectProperty rdf:about="http://storeconnect/storeConnectObjectProperty"/>
    


    <!-- http://storeconnect/with -->

    <owl:ObjectProperty rdf:about="http://storeconnect/with">
        <rdfs:subPropertyOf rdf:resource="http://storeconnect/storeConnectObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://storeconnect/accuracy -->

    <owl:DatatypeProperty rdf:about="http://storeconnect/accuracy">
        <rdfs:subPropertyOf rdf:resource="http://storeconnect/storeConnectDataProperty"/>
        <rdfs:domain rdf:resource="http://storeconnect/Accurate"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    </owl:DatatypeProperty>
    


    <!-- http://storeconnect/building -->

    <owl:DatatypeProperty rdf:about="http://storeconnect/building">
        <rdfs:subPropertyOf rdf:resource="http://storeconnect/storeConnectDataProperty"/>
        <rdfs:domain rdf:resource="http://storeconnect/Location"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    </owl:DatatypeProperty>
    


    <!-- http://storeconnect/floor -->

    <owl:DatatypeProperty rdf:about="http://storeconnect/floor">
        <rdfs:subPropertyOf rdf:resource="http://storeconnect/storeConnectDataProperty"/>
        <rdfs:domain rdf:resource="http://storeconnect/Location"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    </owl:DatatypeProperty>
    


    <!-- http://storeconnect/identifier -->

    <owl:DatatypeProperty rdf:about="http://storeconnect/identifier">
        <rdfs:subPropertyOf rdf:resource="http://storeconnect/storeConnectDataProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- http://storeconnect/orientation -->

    <owl:DatatypeProperty rdf:about="http://storeconnect/orientation">
        <rdfs:subPropertyOf rdf:resource="http://storeconnect/storeConnectDataProperty"/>
        <rdfs:domain rdf:resource="http://storeconnect/Orientation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    </owl:DatatypeProperty>
    


    <!-- http://storeconnect/speed -->

    <owl:DatatypeProperty rdf:about="http://storeconnect/speed">
        <rdfs:subPropertyOf rdf:resource="http://storeconnect/storeConnectDataProperty"/>
        <rdfs:domain rdf:resource="http://storeconnect/Speed"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    </owl:DatatypeProperty>
    


    <!-- http://storeconnect/storeConnectDataProperty -->

    <owl:DatatypeProperty rdf:about="http://storeconnect/storeConnectDataProperty"/>
    


    <!-- http://storeconnect/totalAmount -->

    <owl:DatatypeProperty rdf:about="http://storeconnect/totalAmount">
        <rdfs:subPropertyOf rdf:resource="http://storeconnect/storeConnectDataProperty"/>
        <rdfs:domain rdf:resource="http://storeconnect/Ticket"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://storeconnect/Accurate -->

    <owl:Class rdf:about="http://storeconnect/Accurate">
        <rdfs:subClassOf rdf:resource="http://storeconnect/AccurateThing"/>
        <terms:description xml:lang="en">Use to represent something accurate. Any Accurate is associated with the numeracal value of the accuracy.</terms:description>
    </owl:Class>
    


    <!-- http://storeconnect/AccurateThing -->

    <owl:Class rdf:about="http://storeconnect/AccurateThing">
        <rdfs:subClassOf rdf:resource="http://storeconnect/TechnicalThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://storeconnect/accuracy"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <terms:description xml:lang="en">A owl:Thing related to the StoreConnect&apos;s Accurate notion.</terms:description>
    </owl:Class>
    


    <!-- http://storeconnect/BehavorialHumanCharacteristic -->

    <owl:Class rdf:about="http://storeconnect/BehavorialHumanCharacteristic">
        <rdfs:subClassOf rdf:resource="http://storeconnect/HumanCharacteristic"/>
        <terms:description xml:lang="en">A behavorial characteristic to describe a Human.</terms:description>
    </owl:Class>
    


    <!-- http://storeconnect/BleBeacon -->

    <owl:Class rdf:about="http://storeconnect/BleBeacon">
        <rdfs:subClassOf rdf:resource="http://storeconnect/SensorThing"/>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/Platform"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/sosa/hosts"/>
                <owl:allValuesFrom rdf:resource="http://storeconnect/BleTracker"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <terms:description xml:lang="en">A sosa:Platform for Bluetooh Low Energy devices.</terms:description>
    </owl:Class>
    


    <!-- http://storeconnect/BleTracker -->

    <owl:Class rdf:about="http://storeconnect/BleTracker">
        <rdfs:subClassOf rdf:resource="http://storeconnect/SensorThing"/>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/Sensor"/>
        <terms:description xml:lang="en">A sosa:Sensor of a BleBeacon used to track MotionEvent.</terms:description>
    </owl:Class>
    


    <!-- http://storeconnect/BusinessThing -->

    <owl:Class rdf:about="http://storeconnect/BusinessThing">
        <rdfs:subClassOf rdf:resource="http://storeconnect/StoreConnectThing"/>
        <terms:description xml:lang="en">A owl:Thing related to the business part of the StoreConnect project.</terms:description>
    </owl:Class>
    


    <!-- http://storeconnect/CEL -->

    <owl:Class rdf:about="http://storeconnect/CEL">
        <rdfs:subClassOf rdf:resource="http://storeconnect/CELThing"/>
        <rdfs:subClassOf rdf:resource="http://storeconnect/MotionSubject"/>
        <terms:description xml:lang="en">A CEL (Capacite d&apos;Emport Logistique in french) is the object that can be eventually owned by a MotionSubject when browsing a Route in a Store.</terms:description>
    </owl:Class>
    


    <!-- http://storeconnect/CELThing -->

    <owl:Class rdf:about="http://storeconnect/CELThing">
        <rdfs:subClassOf rdf:resource="http://storeconnect/SubjectThing"/>
        <terms:description xml:lang="en">A owl:Thing related to the CEL notion.</terms:description>
    </owl:Class>
    


    <!-- http://storeconnect/CaisseEnregistreuse -->

    <owl:Class rdf:about="http://storeconnect/CaisseEnregistreuse">
        <rdfs:subClassOf rdf:resource="http://storeconnect/CaisseThing"/>
        <rdfs:subClassOf rdf:resource="http://storeconnect/SensorThing"/>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/Platform"/>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/Sensor"/>
    </owl:Class>
    


    <!-- http://storeconnect/CaisseThing -->

    <owl:Class rdf:about="http://storeconnect/CaisseThing">
        <rdfs:subClassOf rdf:resource="http://storeconnect/BusinessThing"/>
    </owl:Class>
    


    <!-- http://storeconnect/Camera -->

    <owl:Class rdf:about="http://storeconnect/Camera">
        <rdfs:subClassOf rdf:resource="http://storeconnect/SensorThing"/>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/Platform"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/sosa/hosts"/>
                <owl:allValuesFrom rdf:resource="http://storeconnect/VideoTracker"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <terms:description xml:lang="en">A sosa:Platform for Camera devices</terms:description>
    </owl:Class>
    


    <!-- http://storeconnect/Human -->

    <owl:Class rdf:about="http://storeconnect/Human">
        <rdfs:subClassOf rdf:resource="http://storeconnect/HumanThing"/>
        <rdfs:subClassOf rdf:resource="http://storeconnect/MotionSubject"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://storeconnect/hasHumanCharacteristic"/>
                <owl:someValuesFrom rdf:resource="http://storeconnect/HumanCharacteristic"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <terms:description xml:lang="en">A human being, likes any other human being.</terms:description>
    </owl:Class>
    


    <!-- http://storeconnect/HumanCharacteristic -->

    <owl:Class rdf:about="http://storeconnect/HumanCharacteristic">
        <rdfs:subClassOf rdf:resource="http://storeconnect/HumanThing"/>
        <terms:description xml:lang="en">A physical or behavorial characteristic to describe a Human.</terms:description>
    </owl:Class>
    


    <!-- http://storeconnect/HumanThing -->

    <owl:Class rdf:about="http://storeconnect/HumanThing">
        <rdfs:subClassOf rdf:resource="http://storeconnect/SubjectThing"/>
        <terms:description xml:lang="en">A owl:Thing related to the Human notion.</terms:description>
    </owl:Class>
    


    <!-- http://storeconnect/IdentifiableThing -->

    <owl:Class rdf:about="http://storeconnect/IdentifiableThing">
        <rdfs:subClassOf rdf:resource="http://storeconnect/TechnicalThing"/>
        <terms:description xml:lang="en">A owl:Thing that can be identifiable in the StoreConnect project.</terms:description>
    </owl:Class>
    


    <!-- http://storeconnect/Identifier -->

    <owl:Class rdf:about="http://storeconnect/Identifier">
        <rdfs:subClassOf rdf:resource="http://storeconnect/IdentifiableThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://storeconnect/identifier"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <terms:description xml:lang="en">A generic identifier to a owl:Thing.</terms:description>
    </owl:Class>
    


    <!-- http://storeconnect/KnownIdentifier -->

    <owl:Class rdf:about="http://storeconnect/KnownIdentifier">
        <rdfs:subClassOf rdf:resource="http://storeconnect/Identifier"/>
        <terms:description xml:lang="en">A specific identifier that represents a owl:Thing that is already known. For instance, a customer which is already known by a Store.</terms:description>
    </owl:Class>
    


    <!-- http://storeconnect/Location -->

    <owl:Class rdf:about="http://storeconnect/Location">
        <rdfs:subClassOf rdf:resource="http://storeconnect/Accurate"/>
        <rdfs:subClassOf rdf:resource="http://storeconnect/SpatialThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://storeconnect/isWithin"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://storeconnect/StoreArea"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://storeconnect/hasPoint"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.opengis.net/ont/sf#Point"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://storeconnect/building"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://storeconnect/floor"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <terms:description xml:lang="en">A location within a StoreArea. Represented by a geosparql:Point, a floor identifier and a building identifier.</terms:description>
    </owl:Class>
    


    <!-- http://storeconnect/Motion -->

    <owl:Class rdf:about="http://storeconnect/Motion">
        <rdfs:subClassOf rdf:resource="http://storeconnect/MotionThing"/>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/ObservableProperty"/>
        <terms:description xml:lang="en">A Motion is a specific observation type of a Sensor. A Motion represents a move of a Subject. The observation result of a Move is a MotionEvent.</terms:description>
    </owl:Class>
    


    <!-- http://storeconnect/MotionEvent -->

    <owl:Class rdf:about="http://storeconnect/MotionEvent">
        <rdfs:subClassOf rdf:resource="http://storeconnect/MotionThing"/>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/Result"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://storeconnect/hasLocation"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://storeconnect/Location"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://storeconnect/hasMotionState"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://storeconnect/MotionState"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://storeconnect/hasMotionSubject"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://storeconnect/MotionSubject"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://storeconnect/hasOrientation"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://storeconnect/Orientation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://storeconnect/hasSpeed"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://storeconnect/Speed"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <terms:description xml:lang="en">The result of a Motion observation by a Sensor.</terms:description>
    </owl:Class>
    


    <!-- http://storeconnect/MotionState -->

    <owl:Class rdf:about="http://storeconnect/MotionState">
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://storeconnect/Stopping"/>
                    <rdf:Description rdf:about="http://storeconnect/Walking"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://storeconnect/MotionThing"/>
        <terms:description xml:lang="en">A state of a Motion. Can be either Stopping (i.e. no move) or Running (i.e. moving).</terms:description>
    </owl:Class>
    


    <!-- http://storeconnect/MotionSubject -->

    <owl:Class rdf:about="http://storeconnect/MotionSubject">
        <rdfs:subClassOf rdf:resource="http://storeconnect/MotionThing"/>
        <rdfs:subClassOf rdf:resource="http://storeconnect/Subject"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://storeconnect/browses"/>
                <owl:someValuesFrom rdf:resource="http://storeconnect/Route"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <terms:description xml:lang="en">A Subject of a Motion observation made by a Sensor.</terms:description>
    </owl:Class>
    


    <!-- http://storeconnect/MotionThing -->

    <owl:Class rdf:about="http://storeconnect/MotionThing">
        <rdfs:subClassOf rdf:resource="http://storeconnect/BusinessThing"/>
        <terms:description xml:lang="en">A owl:Thing related to the StoreConnect&apos;s Motion notion.</terms:description>
    </owl:Class>
    


    <!-- http://storeconnect/Orientation -->

    <owl:Class rdf:about="http://storeconnect/Orientation">
